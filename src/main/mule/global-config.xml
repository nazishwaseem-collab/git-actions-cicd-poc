	<configuration-properties doc:name="Configuration properties" doc:id="4031e55a-1fd0-47f5-bca1-172d243a81c6" file="properties/config-${env}.yaml" />
	<secure-properties:config name="Secure_Properties_Config" doc:name="Secure Properties Config" doc:id="ee716526-6a92-4658-927a-68de6f4ec26a" file="properties/config-secure-${env}.yaml" key="${mule.key}" />
	<api-gateway:autodiscovery apiId="${api.id}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="21c245fb-062b-43ed-be8e-754fa225f3ea" flowRef="git-actions-cicd-poc-main" />
	
	
</mule>
